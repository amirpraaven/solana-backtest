{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "./build.sh"
  },
  "deploy": {
    "startCommand": "uvicorn src.web.app:app --host 0.0.0.0 --port $PORT",
    "healthcheckPath": "/health/simple",
    "healthcheckTimeout": 30
  }
}